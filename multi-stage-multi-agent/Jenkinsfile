pipeline {
  agent none
  stages {
    stage('Back-end') {
      agent {
        docker { image 'php:8.3.8-alpine3.20' }
      }
      steps {
        sh 'php --version'
      }
    }
    stage('Front-end') {
      agent {
        docker { image 'golang:1.23rc1-alpine3.20' }
      }
      steps {
        sh 'go:1.23rc1 --version'
      }
    }
  }
}

